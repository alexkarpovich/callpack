{
  "name": "ratio",
  "version": "1.0.0",
  "description": "Ratio",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "protractor": "protractor",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "build:dev": "webpack --config config/webpack.dev.js --progress --color --display-error-details --display-cached",
    "build:dev:mock": "webpack --config config/webpack.dev.js --progress --color --display-error-details --display-cached --env.mock",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --color --display-error-details --display-cached --bail",
    "build:prod:mock": "webpack --config config/webpack.prod.js  --progress --color --display-error-details --display-cached --bail --env.mock",
    "lint": "eslint src",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "pretest": "",
    "test": "node --max-old-space-size=4096 node_modules/karma/bin/karma start",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --progress --watch --content-base src/",
    "server:dev:mock": "webpack-dev-server --config config/webpack.dev.js --inline --progress --watch --content-base src/ --env.mock",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "http-server dist --cors",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start:mock": "npm run server:dev:mock"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexkarpovich/ratio.git"
  },
  "author": "Aliaksandr Karpovich",
  "bugs": {
    "url": "https://github.com/alexkarpovich/ratio/issues"
  },
  "homepage": "https://github.com/alexkarpovich/ratio#readme",
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.2",
    "classnames": "~2.2.5",
    "clone": "^1.0.2",
    "compare-versions": "^3.0.0",
    "history": "^1.17.0",
    "jquery": "^2.2.3",
    "moment": "^2.14.1",
    "react": "15.1.0",
    "react-addons-pure-render-mixin": "15.1.0",
    "react-autosuggest": "^3.5.0",
    "react-date-picker": "^5.3.28",
    "react-day-picker": "^2.4.1",
    "react-dom": "15.1.0",
    "react-dropzone-component": "^1.0.4",
    "react-intl": "^2.1.2",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.0",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "react-slider": "^0.6.1",
    "redux": "^3.3.1",
    "redux-form": "~5.3.1",
    "redux-thunk": "^1.0.0",
    "tether": "^1.3.2",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel": "latest",
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-loader": "latest",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.1.18",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.1.2",
    "es6-promise-loader": "^1.0.1",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "eslint": "1.10.3",
    "eslint-plugin-react": "^3.15.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs": "0.0.2",
    "html-webpack-plugin": "^2.15.0",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^3.2.2",
    "react-addons-test-utils": "15.1.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "sass-loader": "^3.2.0",
    "source-map-loader": "^0.1.5",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.13",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.12.0"
  },
  "engines": {
    "node": ">= 7.2.0",
    "npm": ">= 3"
  }
}
